{
    "compilerOptions": {
        "module": "ESNext",
        "moduleResolution": "Node",
        "target": "ES2020",
        "checkJs": true,
        "skipDefaultLibCheck": true,
        "skipLibCheck": true,

        "strictNullChecks": true,
        "strictFunctionTypes": true
    },
    "exclude": [
        "node_modules",
        "**/node_modules/**/*"
    ]
}
